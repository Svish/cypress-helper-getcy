{
  "name": "cypress-helper-getcy",
  "version": "0.0.1",
  "description": "Simple cypress helper for getting elements based on their `data-cy` attribute",
  "keywords": [
    "cypress",
    "helper"
  ],
  "license": "MIT",
  "scripts": {
    "prepare": "tsc",
    "start": "run-s prepare cypress:open",
    "test": "run-s prepare cypress:run",
    "cypress:open": "cypress open --project test",
    "cypress:run": "cypress run --project test",
    "lint": "run-p -slc --aggregate-output lint:*",
    "lint:types": "tsc",
    "lint:prettier": "prettier --ignore-path .gitignore --check \"**/*.{ts,js,json}\""
  },
  "author": "Torleif Berger <torleif@outlook.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/svish/cypress-helper-getcy"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "cypress": ">=0.20.0"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.1.0",
    "cypress": "^3.3.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2",
    "webpack": "^4.35.2"
  }
}
